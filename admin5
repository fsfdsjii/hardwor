print("Welcome! to Hunter's admin say :cmds for commands to choose from")
local cmd = ":cmds"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
		print(":sparkles [user] ")
		print(":remove [user] ")
		print(":fire [user] ")
		print(":highlight [user] ")
		print(":ff [user] ")
		print(":noff [user] ")
		print(":smoke [user] ")
		print(":nosmoke [user] ")
		print(":nofire [user] ")
		print(":nosparkles [user] ")
		print(":clearworkspace")
		print(":sword [user] [ONLY WORKS ON Void, Los angeles] ")
		print(":gun [user] [ONLY WORKS ON Void, Los angeles] ")
		print(":bomb [user] [ONLY WORKS ON Void, Los angeles] ")
		print(":jump [user] ")
	end
end)

local cmd = ":remove"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
		local TargetName=SplitMsg[2]
		local Target=game.Workspace:FindFirstChild(TargetName)
		Target:Destroy()
	end
end)

local cmd = ":sparkles"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
		local TargetName=SplitMsg[2]
		local Target=game.Workspace:FindFirstChild(TargetName)
		local e = Instance.new("Sparkles" , Target.HumanoidRootPart)
		Instance.new("Sparkles" , Target.HumanoidRootPart)
		e.Parent = game.workspace.Target.HumanoidRootPart
	end
end)

local cmd = ":fire"
local seperator = " "

