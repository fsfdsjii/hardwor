print("This is owner rank from Hunter's admin say :cmds for commands to choose from")
local cmd = ":cmds"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
		print(":shutdown")
		print(":kick")
	end
end)

local cmd = ":shutdown"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
game.Players:ClearAllChildren()
	end
end)

local cmd = ":kick"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
Target:Kick("Kicked by owner of Dredter.")
	end
end)
