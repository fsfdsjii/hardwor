	if cmd == SplitMsg[1] then
		local TargetName2=SplitMsg[3]
		local TargetName=SplitMsg[2]
		local Target=game.Workspace:FindFirstChild(TargetName)
		local Target2=game.Workspace:FindFirstChild(TargetName2)
		ett = Instance.new("RopeConstraint")
		ett.Parent = Target.HumanoidRootPart
		local tyy = Instance.new("Attachment")
		tyy.Parent = Target.HumanoidRootPart
		local ey = Instance.new("Attachment")
		ey.Parent = Target2.HumanoidRootPart
		ett.Attachment0 = tyy
		ett.Attachment1 = ey
		ett.Length = ett.CurrentDistance
		ett.Visible = true
	end
end)

