owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
		local TargetName=SplitMsg[2]
		local Target=game.Workspace:FindFirstChild(TargetName)
		Target.Head.BrickColor = BrickColor.new('Really black')
		Target.LeftFoot.BrickColor = BrickColor.new('Really black')
		Target.LeftHand.BrickColor = BrickColor.new('Really black')
		Target.LeftLowerArm.BrickColor = BrickColor.new('Really black')
		Target.RightLowerArm.BrickColor = BrickColor.new('Really black')
		Target.LeftLowerLeg.BrickColor = BrickColor.new('Really black')
		Target.LeftUpperLeg.BrickColor = BrickColor.new('Really black')
		Target.LeftUpperArm.BrickColor = BrickColor.new('Really black')
		Target.LowerTorso.BrickColor = BrickColor.new('Really black')
		Target.RightFoot.BrickColor = BrickColor.new('Really black')
		Target.RightHand.BrickColor = BrickColor.new('Really black')
		Target.RightLowerArm.BrickColor = BrickColor.new('Really black')
		Target.RightLowerLeg.BrickColor = BrickColor.new('Really black')
		Target.RightUpperArm.BrickColor = BrickColor.new('Really black')
		Target.RightUpperLeg.BrickColor = BrickColor.new('Really black')
		Target.UpperTorso.BrickColor = BrickColor.new('Really black')
	end
end)

