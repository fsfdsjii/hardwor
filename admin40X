local cmd = ":nofallwtr"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
		script.Fall:Destroy()
	end
end)

local cmd = ":clone"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
		local TargetName=SplitMsg[2]
		local Target=game.Players:FindFirstChild(TargetName)
		if Target.Character.Humanoid.RigType == Enum.HumanoidRigType.R6 then
Target.Character.Archivable = true
nm = Target.Character:Clone() nm.Parent = workspace
elseif Target.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
print("Character's RigType must be R6.")
end
	end
end)
