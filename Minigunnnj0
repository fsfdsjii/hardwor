Tool0.Name = "6 gold chain guns"
Tool0.Parent = mas
Tool0.TextureId = "rbxassetid://5136268585"
Tool0.Grip = CFrame.new(-1.02309036, 0.854710579, 0.294586182, 1, 2.27373675e-13, -3.72910327e-26, -2.27373675e-13, 1, -1.64007696e-13, 0, 1.64007696e-13, 1)
