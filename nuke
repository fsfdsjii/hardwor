function Enter()
if Input == Code then
print("Entered")
Input = ""

local bomb = script.Parent.Parent
local bo = script.Parent
bomb:makeJoints()
bomb.L.Anchored = false
bomb.R.Anchored = false
bomb.A.Anchored = false
bomb.S.Anchored = false
bomb.D.Anchored = false
bomb.F.Anchored = false
bomb.SliderL.Anchored = false
bomb.SliderR.Anchored = false
wait()
bomb.SliderL.BodyVelocity.velocity = Vector3.new(0, -1, 0)
bomb.SliderR.BodyVelocity.velocity = Vector3.new(0, 1, 0)
bomb.Name = "Code Accepted. Nuke deactivated."
script.Parent.Parent.Parent.Boom:remove()
script.Parent.Parent.Parent.Name = "Disabled"
return end
Input = ""
print("Code is - 123789456")
end




script.Parent.Enter.ClickDetector.MouseClick:connect(Enter)

