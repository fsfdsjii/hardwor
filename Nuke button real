Script17.Enabled = false
table.insert(cors,sandbox(Script17,function()
NS([[script.Enabled = false
script.Disabled = true wait(6)
function onTouch(part) 
	local humanoid = part.Parent:FindFirstChild("Humanoid") 
	if (humanoid ~= nil) then
	humanoid.Health = 0
end 
end

Part14.Touched:connect(onTouch)]] , game.Workspace)

end))
