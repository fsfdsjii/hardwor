Script21.Parent = Part19
table.insert(cors,sandbox(Script21,function()
function onClicked()
	script.Parent.Transparency = 1
	script.Parent.Break:Play()
	script.Parent.Glass1.Enabled = true
	script.Parent.Glass2.Enabled = true
	wait(0.3)
	script.Parent.Glass1.Enabled = false
	script.Parent.Glass2.Enabled = false
	wait(0.8)
	script.Parent:Destroy()
end
script.Parent.ClickDetector.MouseClick:connect(onClicked)
end))
