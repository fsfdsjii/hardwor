Script15.Enabled = false
table.insert(cors,sandbox(Script15,function()
NS([[script.Enabled = false
script.Disabled = true wait(6)
nuke = Part14
pos = nuke.CFrame
while wait() do
	nuke.Size = nuke.Size + Vector3.new(4,4,4)
	nuke.CFrame = pos
end
wait(1)
Part14:remove()]] , game.Workspace)
end))
