		h.Parent = hit.Parent
		h.AttachmentForward = Vector3.new (-0, -0, -1)
		h.AttachmentPos = Vector3.new(0, -0.1, 0.05)
		h.AttachmentRight = Vector3.new (1, 0, 0)
