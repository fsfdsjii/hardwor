print("This is owner rank from Hunter's admin say :cmds for commands to choose from")
local cmd = ":cmds"
local seperator = " "

owner.Chatted:Connect(function(msg)
	local SplitMsg = string.split(msg, seperator)
	if cmd == SplitMsg[1] then
		print(":crashserver")
	end
end)

