Sound1003.SoundId = "rbxassetid://12221990"
Sound1003.Volume = 1
Script1004.Name = "Reset"
Script1004.Parent = UnionOperation1002
table.insert(cors,sandbox(Script1004,function()
	script.Parent.BrickColor = BrickColor.new("Mid gray")
	while true do
		wait(120)
		if script.Parent:FindFirstChild("Count") and script.Parent:FindFirstChild("Count"):IsA("Sound") then
			script.Parent.Count:Play()
		end
		script.Parent.BrickColor = BrickColor.new("Dark stone grey")
		wait(60)
		if script.Parent:FindFirstChild("Count") and script.Parent:FindFirstChild("Count"):IsA("Sound") then
			script.Parent.Count:Play()
		end
		script.Parent.BrickColor = BrickColor.new("Black")
		wait(30)
		if script.Parent:FindFirstChild("Count") and script.Parent:FindFirstChild("Count"):IsA("Sound") then
			script.Parent.Count:Play()
		end
		script.Parent.BrickColor = BrickColor.new("Bright red")
		wait(20)
		for i = 1,10 do
			if script.Parent:FindFirstChild("Count") and script.Parent:FindFirstChild("Count"):IsA("Sound") then
				script.Parent.Count:Play()
			end
			wait(1)
		end
		script.Parent.BrickColor = BrickColor.new("Mid gray")
		local C = script.Parent.Parent.Tiles:GetChildren()
		for i = 1,30 do
			for i = 1,#C do
				if C[i]:IsA("Part") then
					if C[i]:FindFirstChild("Item") and C[i]:FindFirstChild("Item"):IsA("IntValue") then
						if C[i].Item.Value == 2 then
							if C[i]:FindFirstChild("Checked") and C[i]:FindFirstChild("Checked"):IsA("BoolValue") then
								if C[i].Checked.Value == true then
									C[i].BrickColor = BrickColor.new("Really blue")
								elseif C[i].Checked.Value == false then
									C[i].BrickColor = BrickColor.new("Really red")
								end
							end
						end
					end
				end
			end
			wait(0.3)
			for i = 1,#C do
				if C[i]:IsA("Part") then
					if C[i]:FindFirstChild("Item") and C[i]:FindFirstChild("Item"):IsA("IntValue") then
						if C[i].Item.Value == 2 then
							if C[i]:FindFirstChild("Checked") and C[i]:FindFirstChild("Checked"):IsA("BoolValue") then
								if C[i].Checked.Value == true then
									C[i].BrickColor = BrickColor.new("Really black")
								elseif C[i].Checked.Value == false then
									C[i].BrickColor = BrickColor.new("Hot pink")
								end
							end
						end
					end
				end
			end
			wait(0.3)
		end
		for i = 1,#C do
			if C[i]:FindFirstChild("MinesweeperScript") and C[i]:FindFirstChild("MinesweeperScript"):IsA("Script") then
				C[i].MinesweeperScript.Disabled = true
			end
		end
		wait(3)
		for i = 1,#C do
			if C[i]:FindFirstChild("MinesweeperScript") and C[i]:FindFirstChild("MinesweeperScript"):IsA("Script") then
				C[i].MinesweeperScript.Disabled = false
			end
		end
	end
end))
for i,v in pairs(mas:GetChildren()) do
	v.Parent = script
	pcall(function() v:MakeJoints() end)
end
mas:Destroy()
for i,v in pairs(cors) do
	spawn(function()
		pcall(v)
	end)
end
