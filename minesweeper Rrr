					Bang.Name = "MinesweeperExplosion"
					Bang.BlastRadius = 18 * ((Tile.Size.X + Tile.Size.Y + Tile.Size.Z / 3) / 3)
					Bang.BlastPressure = 1000000000000000000000000000000
					Bang.ExplosionType = Enum.ExplosionType.CratersAndDebris
					Bang.Position = Tile.Position
					if Tile:FindFirstChild("Boom") and Tile:FindFirstChild("Boom"):IsA("Sound") then
						Tile.Boom:Play()
					end
					wait(0.1)
				end
			end
		end
	end
	Tile.Touched:connect(Contact)
