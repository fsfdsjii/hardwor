	Tile.Material = Enum.Material.Metal
	Checked.Value = false
	TileItem.Value = 0
	Count.Value = 0
	local Value = math.random(1,5)
	if Value <= 4 then
		TileItem.Value = 1 -- Regular
	else
		TileItem.Value = 2 -- Mine
	end
	function Counted()
		if Count.Value == 1 then
			Tile.BrickColor = BrickColor.new("Medium stone grey")
		elseif Count.Value == 2 then
			Tile.BrickColor = BrickColor.new("Royal purple")
		elseif Count.Value == 3 then
			Tile.BrickColor = BrickColor.new("Really blue")
		elseif Count.Value == 4 then
			Tile.BrickColor = BrickColor.new("Lime green")
		elseif Count.Value == 5 then
			Tile.BrickColor = BrickColor.new("New Yeller")
		elseif Count.Value == 6 then
			Tile.BrickColor = BrickColor.new("Neon orange")
		elseif Count.Value == 7 then
			Tile.BrickColor = BrickColor.new("Really red")
		elseif Count.Value >= 8 then
			Tile.BrickColor = BrickColor.new("Dark stone grey")
		end
	end
	Count.Changed:connect(Counted)
	function BlastHit(item)
		if item.Name == "Tile" then
			if item:FindFirstChild("Item") then
				if item:FindFirstChild("Item"):IsA("IntValue") then
					if item.Item.Value == 2 then
						Tile.Count.Value = Tile.Count.Value + 1
					end
				end
			end
		end
	end
	function Contact()
		if Checked.Value == false then
			if TileItem.Value == 1 then
				Checked.Value = true
				Tile.BrickColor = BrickColor.new("White")
				Tile.Material = Enum.Material.SmoothPlastic
				if Tile:FindFirstChild("PressSafe") and Tile:FindFirstChild("PressSafe"):IsA("Sound") then
					Tile.PressSafe:Play()
				end
				wait(0.3)
				if Tile:FindFirstChild("Okay") and Tile:FindFirstChild("Okay"):IsA("Sound") then
					Tile.Okay.PlaybackSpeed = 1
					Tile.Okay:Play()
				end
				local Bang = Instance.new("Explosion",script.Parent)
				Bang.Name = "Checking"
				Bang.Visible = false
				Bang.BlastRadius = 9 * ((Tile.Size.X + Tile.Size.Y + Tile.Size.Z / 3) / 3)
				Bang.BlastPressure = 0
				Bang.DestroyJointRadiusPercent = 0
				Bang.ExplosionType = Enum.ExplosionType.NoCraters
				Bang.Position = Tile.Position
				Bang.Hit:connect(BlastHit)
				wait(0.5)
				for i = 1,Tile.Count.Value do
					wait(0.3)
					if Tile:FindFirstChild("Okay") and Tile:FindFirstChild("Okay"):IsA("Sound") then
						Tile.Okay.PlaybackSpeed = 4
						Tile.Okay:Play()
					end
				end
			elseif TileItem.Value == 2 then
				Checked.Value = true
				Tile.Material = Enum.Material.Neon
				if Tile:FindFirstChild("PressUhOh") and Tile:FindFirstChild("PressUhOh"):IsA("Sound") then
					Tile.PressUhOh:Play()
				end
				for i = 1,8 do
					Tile.BrickColor = BrickColor.new("Really red")
					if Tile:FindFirstChild("Beep") and Tile:FindFirstChild("Beep"):IsA("Sound") then
						Tile.Beep:Play()
					end
					wait(0.05)
					Tile.BrickColor = BrickColor.new("Really black")
					wait(0.05)
				end
				for i = 1,5 do
					local Bang = Instance.new("Explosion",script.Parent)
					Bang.Name = "MinesweeperExplosion"
					Bang.BlastRadius = 18 * ((Tile.Size.X + Tile.Size.Y + Tile.Size.Z / 3) / 3)
					Bang.BlastPressure = 1000000000000000000000000000000
					Bang.ExplosionType = Enum.ExplosionType.CratersAndDebris
					Bang.Position = Tile.Position
					if Tile:FindFirstChild("Boom") and Tile:FindFirstChild("Boom"):IsA("Sound") then
						Tile.Boom:Play()
					end
					wait(0.1)
				end
			end
		end
	end
	Tile.Touched:connect(Contact)
end))
