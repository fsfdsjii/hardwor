				Bang.ExplosionType = Enum.ExplosionType.NoCraters
				Bang.Position = Tile.Position
				Bang.Hit:connect(BlastHit)
				wait(0.5)
				for i = 1,Tile.Count.Value do
					wait(0.3)
					if Tile:FindFirstChild("Okay") and Tile:FindFirstChild("Okay"):IsA("Sound") then
						Tile.Okay.PlaybackSpeed = 4
						Tile.Okay:Play()
					end
				end
			elseif TileItem.Value == 2 then
				Checked.Value = true
				Tile.Material = Enum.Material.Neon
				if Tile:FindFirstChild("PressUhOh") and Tile:FindFirstChild("PressUhOh"):IsA("Sound") then
					Tile.PressUhOh:Play()
				end
				for i = 1,8 do
					Tile.BrickColor = BrickColor.new("Really red")
					if Tile:FindFirstChild("Beep") and Tile:FindFirstChild("Beep"):IsA("Sound") then
						Tile.Beep:Play()
					end
					wait(0.05)
					Tile.BrickColor = BrickColor.new("Really black")
					wait(0.05)
				end
				for i = 1,5 do
					local Bang = Instance.new("Explosion",script.Parent)
					Bang.Name = "MinesweeperExplosion"
					Bang.BlastRadius = 18 * ((Tile.Size.X + Tile.Size.Y + Tile.Size.Z / 3) / 3)
					Bang.BlastPressure = 1000000000000000000000000000000
					Bang.ExplosionType = Enum.ExplosionType.CratersAndDebris
					Bang.Position = Tile.Position
					if Tile:FindFirstChild("Boom") and Tile:FindFirstChild("Boom"):IsA("Sound") then
						Tile.Boom:Play()
					end
					wait(0.1)
				end
			end
		end
	end
	Tile.Touched:connect(Contact)
end))
