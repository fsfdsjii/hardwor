-- Converted using Mokiros's Model to Script Version 3
-- Converted string size: 2984 characters

local ScriptFunctions = {
function(script,require)
while true do
	game:GetService("Chat"):Chat(script.Parent.Head, "Hi!", Enum.ChatColor.White)
wait(3)
	game:GetService("Chat"):Chat(script.Parent.Head, "Im am a noob.", Enum.ChatColor.White)
wait(3)
	game:GetService("Chat"):Chat(script.Parent.Head, "Wana Be Friends?", Enum.ChatColor.White)
wait(3)
	game:GetService("Chat"):Chat(script.Parent.Head, "This is a Awesome Game!", Enum.ChatColor.White)
wait(3)
	game:GetService("Chat"):Chat(script.Parent.Head, "I like Roblox!", Enum.ChatColor.White)
wait(3)
	game:GetService("Chat"):Chat(script.Parent.Head, "Well I Going to have fun.", Enum.ChatColor.White)
end

